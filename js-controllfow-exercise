console.log(b)
let c = '\xa0\xa0\xa0\xa0' + '^^^^^' + '\xa0\xa0\xa0\xa0'
console.log(c)
let d = '\xa0\xa0\xa0' + '^^^^^^^' + '\xa0\xa0\xa0'
console.log(d)
let e = '\xa0\xa0' + '^^^^^^^^^' + '\xa0\xa0'
console.log(e)
let f = '\xa0' + '^^^^^^^^^^^' + '\xa0'
console.log(f)


